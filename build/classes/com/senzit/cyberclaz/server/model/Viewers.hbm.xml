<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.senzit.cyberclaz.server.model.Viewers" table="viewers">
	<meta attribute="class-description">
		This class contains the user note detail.
	</meta>
		<id name="viewerId" column="VIEWERID">
			<generator class="increment"></generator>
        </id>
     	<many-to-one name="user" column="USERID" class="com.senzit.cyberclaz.server.model.User">	
     	</many-to-one>
     	<many-to-one name="classEventDetail" column="CLASSEVENTDETAILID" class="com.senzit.cyberclaz.server.model.ClassEventDetail">	
     	</many-to-one>
	    <property name="userRateCount" column="USERRATECOUNT" type="integer"/>
	      <property name="teacherName" column="TEACHERNAME" type="string"/>
	</class>
</hibernate-mapping>