<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.senzit.cyberclaz.server.model.Privilege" table="privilege">
	<meta attribute="class-description">
		This class contains the privilege detail.
	</meta>
		<id name="privilegeId" column="PRIVILEGEID">
        </id>
		<property name="privilegeName" column="PRIVILEGENAME" type="string"/>
	</class>
</hibernate-mapping>