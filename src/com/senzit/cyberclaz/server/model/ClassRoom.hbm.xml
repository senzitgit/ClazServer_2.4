<hibernate-mapping>
<class name="com.senzit.cyberclaz.server.model.ClassRoom" table="classroom">
	<meta attribute="class-description">
		This class contains the class room detail.
	</meta>
		<id name="classRoomId" column="CLASSROOMID">
        </id>
     	<property name="classRoomNo" column="CLASSROOMNO" type="string" unique="true"/>
	</class>
</hibernate-mapping>