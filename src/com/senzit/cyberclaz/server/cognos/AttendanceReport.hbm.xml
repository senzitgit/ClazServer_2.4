<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.senzit.cyberclaz.server.cognos.AttendanceReport" table="attendancereport">
	<meta attribute="class-description">
		This class contains the user login detail.
	</meta>
		<id name="userId" column="USERID">
            <generator class="foreign">
                <param name="property">user</param>
            </generator>
        </id>
        <one-to-one name="user" class="com.senzit.cyberclaz.server.model.User"
        		constrained="true">	
     	</one-to-one>
     	<property name="term" column="TERM" type="string"/>
       	<property name="month" column="MONTH" type="string"/>
        <property name="percentage" column="PERCENTAGE" type="string"/>			
     	
	</class>
</hibernate-mapping>